{
    "name": "apn/ami-client",
    "description": "High-performance, non-blocking Asterisk Manager Interface (AMI) client for PHP 8.4+",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Apn\\AmiClient\\": "src/"
        }
    },
    "require": {
        "php": ">=8.4",
        "psr/log": "^3.0",
        "illuminate/support": "^12.0",
        "illuminate/console": "^12.0"
    },
    "require-dev": {
        "phpoption/phpoption": "^1.9",
        "phpunit/phpunit": "^12.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "check:core-boundaries": "php scripts/validate_guidelines.php"
    },
    "minimum-stability": "stable"
}
